{
  "name": "draw-libre",
  "version": "0.3.0",
  "description": "Draw a line/polygon on a maplibre or mapbox map",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:w": "tsup --watch",
    "dev": "tsc -w",
    "lint": "tsc",
    "prettier": "prettier --write .",
    "exports": "attw --pack .",
    "ci": "npm run lint && npm run prettier && npm run build",
    "release": "npm run ci",
    "prepublishOnly": "npm run ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HusikyanLevon/draw-libre.git"
  },
  "keywords": [
    "maplibre",
    "mapbox",
    "draw"
  ],
  "author": "Usikian Levon <levonhusikyan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HusikyanLevon/draw-libre/issues"
  },
  "homepage": "https://github.com/HusikyanLevon/draw-libre#readme",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "mapbox-gl": "^3",
    "maplibre-gl": "^3"
  },
  "devDependencies": {
    "@types/mapbox-gl": "3.1.0",
    "@types/node": "20.12.8",
    "prettier": "3.2.5",
    "tsup": "8.2.2",
    "typescript": "5.4.5"
  },
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "style": "dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "sideEffects": false
}
