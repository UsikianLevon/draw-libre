{
  "display": "Maplibre Draw",
  "_version": "20.1.0",
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["DOM", "es2023"],
    "target": "es2022",
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "strict": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "allowJs": true,
    "noEmit": false,
    "module": "esnext",
    "resolveJsonModule": true,
    "outDir": "dist",
    "rootDir": "src",
    "sourceMap": true,
    "noImplicitAny": true,
    "moduleResolution": "bundler",
    "paths": {
      "#components/*": ["src/components/*"],
      "#store/*": ["src/store/*"],
      "#utils/*": ["src/utils/*"],
      "#types/*": ["src/types/*"]
    }
  },
  "include": ["src"]
}
